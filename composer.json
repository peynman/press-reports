{
    "name": "peynman/larapress-reports",
    "description": "Larapress Reports.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [{
        "name": "Peyman Abdi",
        "email": "peyman.abdi.work@gmail.com"
    }],
    "autoload": {
        "psr-4": {
            "Larapress\\Reports\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larapress\\Reports\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larapress\\Reports\\Providers\\PackageServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.3.0",
        "laravel/framework": "^8.0",
        "peynman/larapress-crud": "master",
        "influxdata/influxdb-client-php": "master"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "orchestra/testbench": "^6.2.0",
        "phpunit/phpunit": "^9.0",
        "phpmd/phpmd": "@stable",
        "squizlabs/php_codesniffer": "3.*"
    },
    "repositories": {
        "influxdata/influxdb-client-php":{
            "type": "vcs",
            "url": "https://github.com/peynman/influxdb-client-php"
        },
        "peynman/larapress-crud": {
            "type": "path",
            "url": "../press-crud",
            "options": {
                "symlink": true
            }
        }
    }
}
