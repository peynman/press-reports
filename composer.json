{
    "name": "peynman/larapress-reports",
    "description": "Larapress default dashboard.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [{
        "name": "Peyman Abdi",
        "email": "peyman.abdi.work@gmail.com"
    }],
    "autoload": {
        "psr-4": {
            "Larapress\\Reports\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larapress\\Reports\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larapress\\Reports\\Providers\\PackageServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.2.5",
        "laravel/framework": "^7.0",
        "peynman/larapress-crud": "master",
        "influxdb/influxdb-php": "^1.15"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "orchestra/testbench": "^4.4",
        "phpunit/phpunit": "^8.0",
        "phpmd/phpmd": "@stable",
        "squizlabs/php_codesniffer": "3.*",
        "matthewbdaly/artisan-standalone": "dev-master"
    }
}
